{
    "*.{js,jsx}": [
        "npx prettier --write '*.{js,jsx}'",
        "node scripts/lint.js --js --fix --only --"
    ],
    "*.{ts,tsx}": [
        "npx eslint --fix",
        "npx prettier --write '*.{ts,tsx}'"
    ],
    "*.{js,ts,d.ts}": [
        "npx prettier --write '*.{js,ts,d.ts}'"
    ],
    "{!CHANGELOG.md, *.md}": [
        "npx prettier --write '{!CHANGELOG.md, *.md}'"
    ],
    "*.{gn,gni}": [
        "npm run gn-check",
        "npm run gn-format"
    ],
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "*.css": [
        "stylelint --fix",
        "prettier --write '*.css'"
    ],
    "{*.patch,.patches}": [
        "node scripts/lint.js --patches --only --",
        "ts-node scripts/js/check-patch-diff.ts"
    ]
}